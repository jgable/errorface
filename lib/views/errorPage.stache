        (function($, hljs) {

            $("code").each(function() {
                hljs.highlightBlock(this);
            });

            var $stack = $("#stack"),
                $currSnoop = $("#snoops .level").first().toggleClass("show", true),
                $currSlice = $stack.find(".slice").first().toggleClass("focus", true);

            $stack.on("click", ".slice", function(e) {
                var $slice = $(e.currentTarget),
                    level = $slice.data("level"),
                    fileHash = $slice.data("filehash");

                $currSlice.toggleClass("focus", false);
                $currSlice = $slice;
                $currSlice.toggleClass("focus", true);
                
                $currSnoop.toggleClass("show", false);
                $currSnoop = $("#level-" + level);
                $currSnoop.toggleClass("show", true);

                // Toggle our muted layers
                var isMuted = true,
                    $this = null;
                $stack.find(".slice").each(function() { 
                    $this = $(this);
                    if ($this.hasClass("focus")) {
                        isMuted = false;
                    }

                    $this.toggleClass("muted", isMuted);

                });

                return false;
            });

            // Build a collection of styles for colors of file names
            var colors = [
                    "#1E90FF",
                    "#CCDF32",
                    "#D197D9",
                    "#BED6FF",
                    "#66CCB3",
                    "#EFB571"
                ], 
                currColorIdx = 0,
                fileHash = {},
                rules = [];

            {{#stack}}{{#trace}}{{#details}}
            fileHash["{{fileHash}}"] = true;
            {{/details}}{{/trace}}{{/stack}}

            for (var key in fileHash) {
                if (!fileHash.hasOwnProperty(key)) { continue; }

                rules.push(".file-" + key + " { color: " + colors[currColorIdx % colors.length] + "; }");
                currColorIdx++;
            }

            var $fileStyles = $("<style type='text/css'>" + rules.join("\n") + "</style>");
            $("body").append($fileStyles);

        }($, hljs));
        
    </script>
